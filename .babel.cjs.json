{
  "$schema": "https://json.schemastore.org/babelrc",
  "targets": {
    "node": "18"
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "cjs"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-export-namespace-from"],
    ["@babel/transform-modules-commonjs"],
    ["annotate-pure-calls"],
    ["@effect/babel-plugin"]
  ]
}
