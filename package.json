{
  "name": "typed-fp",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "A functional programming toolkit for building applications with fp-ts v3",
  "keywords": [
    "TypeScript",
    "FP",
    "Effects"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --target=build:cjs --all",
    "build:affected": "nx affected --target=build --target=build:cjs",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected --target=lint",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all -c=watch",
    "test:affected": "nx affected --target=test",
    "watch": "tsc --build --watch",
    "update": "pnpm run update:rootTsconfig && pnpm run update:packages",
    "update:packages": "tsx ./tools/updatePackages.ts",
    "update:rootTsconfig": "tsx ./tools/updateRootTsconfig.ts",
    "example": "pnpm typed serve -c ./example/vite.config.js",
    "example:build": "pnpm typed build -c ./example/vite.config.js",
    "example:preview": "pnpm typed preview -c ./example/vite.config.js",
    "example:static": "pnpm typed run -s example/static.ts",
    "typed": "pnpm tsx ./packages/cli/src/cli.ts",
    "release": "pnpm run build && lerna publish",
    "~~NEEDS POSITIONAL COMMAND LINE ARGUMENT~~": "",
    "new-package": "nx generate ./packages/nx-plugin:package"
  },
  "dependencies": {
    "@effect/data": "^0.18.3",
    "@effect/io": "^0.39.0",
    "@effect/language-service": "^0.0.21",
    "@effect/platform": "^0.14.0",
    "@effect/platform-node": "^0.14.0",
    "@effect/schema": "^0.34.0",
    "@effect/stream": "^0.35.1",
    "@rollup/pluginutils": "^5.0.4",
    "@vavite/multibuild": "^2.0.2",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "escape-html": "^1.0.3",
    "fast-deep-equal": "^3.1.3",
    "fast-glob": "^3.3.1",
    "happy-dom": "^10.11.2",
    "isbot": "^3.6.13",
    "mock-storage": "^1.1.0",
    "path-to-regexp": "^6.2.1",
    "picocolors": "^1.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "shx": "^0.3.4",
    "ts-morph": "^19.0.0",
    "ts-toolbelt": "^9.6.0",
    "typed-query-selector": "^2.11.0",
    "udomdiff": "^1.1.0",
    "vavite": "^2.0.2",
    "vite": "^4.4.9",
    "vite-node": "^0.34.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@nrwl/devkit": "^16.7.4",
    "@nrwl/linter": "^16.7.4",
    "@nrwl/nx-plugin": "^16.7.4",
    "@nrwl/vite": "16.7.4",
    "@nrwl/workspace": "^16.7.4",
    "@tsconfig/svelte": "^5.0.2",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/browser": "^0.34.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.3",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lerna": "^7.2.0",
    "lint-staged": "^14.0.1",
    "nx": "16.7.4",
    "playwright": "^1.37.1",
    "prettier": "^3.0.3",
    "tslib": "^2.6.2",
    "tsx": "^3.12.8",
    "typescript": "5.1.6",
    "vitest": "0.34.3",
    "webdriverio": "^8.16.0"
  }
}
