{
  "name": "typed-fp",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "A functional programming toolkit for building applications with fp-ts v3",
  "keywords": [
    "TypeScript",
    "FP",
    "Effects"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:affected": "nx affected --target=build",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all -c=fix",
    "lint:affected": "nx affected --target=lint",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all -c=watch",
    "test:affected": "nx affected --target=test",
    "watch": "tsc --build --watch --preserveWatchOutput",
    "update": "pnpm run update:rootTsconfig && pnpm run update:packages",
    "update:packages": "tsx ./tools/updatePackages.ts",
    "update:rootTsconfig": "tsx ./tools/updateRootTsconfig.ts",
    "example": "vite --config ./vite.config.ts serve example",
    "example:build": "vavite --config ./vite.config.ts",
    "example:preview": "node ./example/dist/server/index.js",
    "~~NEEDS POSITIONAL COMMAND LINE ARGUMENT~~": "",
    "new-package": "nx generate ./packages/nx-plugin:package"
  },
  "dependencies": {
    "@effect/io": "~0.0.60",
    "@effect/stm": "~0.0.13",
    "@fp-ts/core": "~0.0.11",
    "@fp-ts/data": "~0.0.36",
    "@fp-ts/schema": "~0.0.7",
    "@webreflection/uparser": "~0.2.4",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "happy-dom": "^8.1.3",
    "isbot": "^3.6.5",
    "morphdom": "^2.6.1",
    "path-to-regexp": "^6.2.1",
    "ts-toolbelt": "^9.6.0",
    "typed-query-selector": "^2.8.0",
    "udomdiff": "^1.1.0",
    "vavite": "^1.5.2",
    "vite-plugin-compression": "^0.5.1",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.4",
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "@nrwl/devkit": "^15.4.5",
    "@nrwl/linter": "^15.4.5",
    "@nrwl/nx-plugin": "^15.4.5",
    "@nrwl/vite": "15.4.5",
    "@nrwl/workspace": "^15.4.5",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/yargs": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.26.3",
    "@vitest/ui": "^0.26.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express-static-gzip": "^2.1.7",
    "husky": "^8.0.3",
    "lerna": "^6.4.0",
    "lint-staged": "^13.1.0",
    "nx": "15.4.5",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0",
    "ts-morph": "^17.0.1",
    "tsconfig-paths": "github:IgnusG/tsconfig-paths#fce7c52ed061417dfd1dcce94721c4a94c230bab",
    "tslib": "^2.4.1",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-node": "^0.26.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-react": "^4.0.1",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.3"
  },
  "pnpm": {
    "overrides": {
      "tsconfig-paths": "git://github.com/IgnusG/tsconfig-paths#fce7c52ed061417dfd1dcce94721c4a94c230bab"
    }
  }
}
