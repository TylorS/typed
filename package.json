{
  "name": "typed-fp",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "A functional programming toolkit for building applications with fp-ts v3",
  "keywords": [
    "TypeScript",
    "FP",
    "Effects"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:affected": "nx affected --target=build",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all -c=fix",
    "lint:affected": "nx affected --target=lint",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all -c=watch",
    "test:affected": "nx affected --target=test",
    "watch": "tsc --build --watch --preserveWatchOutput",
    "update": "pnpm run update:rootTsconfig && pnpm run update:packages",
    "update:packages": "tsx ./tools/updatePackages.ts",
    "update:rootTsconfig": "tsx ./tools/updateRootTsconfig.ts",
    "example": "pnpm typed example",
    "example:build": "pnpm typed build -s ./example/static.ts example",
    "example:preview": "node ./example/dist/server/index.js",
    "typed": "pnpm tsx ./packages/cli/src/cli.ts",
    "~~NEEDS POSITIONAL COMMAND LINE ARGUMENT~~": "",
    "new-package": "nx generate ./packages/nx-plugin:package"
  },
  "dependencies": {
    "@effect/io": "0.1.0",
    "@effect/stm": "0.0.15",
    "@fp-ts/core": "0.1.1",
    "@fp-ts/data": "0.1.0",
    "@fp-ts/schema": "0.1.2",
    "@vavite/multibuild": "^1.5.3",
    "@webreflection/uparser": "~0.2.4",
    "cac": "^6.7.14",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "fast-glob": "^3.2.12",
    "happy-dom": "^8.1.5",
    "html-minifier": "^4.0.0",
    "isbot": "^3.6.5",
    "path-to-regexp": "^6.2.1",
    "picocolors": "^1.0.0",
    "ts-toolbelt": "^9.6.0",
    "typed-query-selector": "^2.8.0",
    "udomdiff": "^1.1.0",
    "vavite": "^1.5.3",
    "vite": "^4.0.4",
    "vite-node": "^0.28.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.0.5",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@effect/language-service": "^0.0.17",
    "@effect/vite-plugin": "^0.0.1",
    "@esbuild-kit/esm-loader": "^2.5.4",
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "@nrwl/devkit": "^15.6.2",
    "@nrwl/linter": "^15.6.2",
    "@nrwl/nx-plugin": "^15.6.2",
    "@nrwl/vite": "15.6.2",
    "@nrwl/workspace": "^15.6.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/express": "^4.17.16",
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.2",
    "@vitest/ui": "^0.28.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.0",
    "nx": "15.6.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-language-server": "^0.15.3",
    "svelte-preprocess": "^5.0.1",
    "ts-morph": "^17.0.1",
    "tslib": "^2.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-react": "^4.0.1",
    "vitest": "^0.28.2"
  }
}
