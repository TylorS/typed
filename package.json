{
  "name": "typed-fp",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "A functional programming toolkit for building applications with fp-ts v3",
  "keywords": [
    "TypeScript",
    "FP",
    "Effects"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --target=build:cjs --all",
    "build:affected": "nx affected --target=build --target=build:cjs",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected --target=lint",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all -c=watch",
    "test:affected": "nx affected --target=test",
    "watch": "tsc --build --watch",
    "update": "pnpm run update:rootTsconfig && pnpm run update:packages",
    "update:packages": "tsx ./tools/updatePackages.ts",
    "update:rootTsconfig": "tsx ./tools/updateRootTsconfig.ts",
    "example": "pnpm typed serve",
    "example:build": "pnpm typed build -s static.ts",
    "example:preview": "pnpm typed preview",
    "example:static": "pnpm typed run -s example/static.ts",
    "typed": "pnpm tsx ./packages/cli/src/cli.ts",
    "release": "pnpm run build && lerna publish",
    "~~NEEDS POSITIONAL COMMAND LINE ARGUMENT~~": "",
    "new-package": "nx generate ./packages/nx-plugin:package"
  },
  "dependencies": {
    "@effect/data": "^0.10.3",
    "@effect/io": "^0.17.0",
    "@effect/language-service": "^0.0.19",
    "@effect/query": "^0.5.0",
    "@effect/schema": "^0.10.0",
    "@rollup/pluginutils": "^5.0.2",
    "@vavite/multibuild": "^1.7.0",
    "@webreflection/uparser": "~0.2.4",
    "cac": "^6.7.14",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "fast-glob": "^3.2.12",
    "happy-dom": "^9.1.0",
    "html-minifier": "^4.0.0",
    "isbot": "^3.6.8",
    "path-to-regexp": "^6.2.1",
    "picocolors": "^1.0.0",
    "ts-morph": "^18.0.0",
    "ts-toolbelt": "^9.6.0",
    "typed-query-selector": "^2.9.2",
    "udomdiff": "^1.1.0",
    "vavite": "^1.7.0",
    "vite": "^4.2.1",
    "vite-node": "^0.29.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.0.7",
    "yargs": "^17.7.1"
  },
  "peerDependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@most/core": "^1.6.1",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "@nrwl/devkit": "^15.9.2",
    "@nrwl/linter": "^15.9.2",
    "@nrwl/nx-plugin": "^15.9.2",
    "@nrwl/vite": "15.9.2",
    "@nrwl/workspace": "^15.9.2",
    "@sveltejs/vite-plugin-svelte": "^2.0.4",
    "@tsconfig/svelte": "^4.0.1",
    "@types/express": "^4.17.17",
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.29.8",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lerna": "^6.6.1",
    "lint-staged": "^13.2.0",
    "nx": "15.9.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.1.4",
    "svelte-language-server": "^0.15.8",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  }
}
