{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "tsc --build"
          }
        ]
      },
      "outputs": [
        "{workspaceRoot}/dist/exit"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/exit/**/*.ts"
        ]
      }
    }
  }
}
