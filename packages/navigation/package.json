{
  "name": "@typed/navigation",
  "version": "0.5.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/cjs/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "dependencies": {
    "@effect/data": "^0.17.1",
    "@effect/io": "^0.38.0",
    "@typed/context": "workspace:*",
    "@typed/dom": "workspace:*",
    "@typed/fx": "workspace:*",
    "@typed/path": "workspace:*",
    "happy-dom": "^10.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^20.4.10",
    "vitest": "0.34.1"
  }
}
