{
  "name": "@typed/nx-plugin",
  "private": true,
  "version": "0.7.2",
  "type": "commonjs",
  "devDependencies": {
    "@nrwl/devkit": "^16.7.4",
    "@types/node": "^20.5.6"
  },
  "generators": "generators.json",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/cjs/*.js",
      "types": "./dist/*.d.ts"
    }
  }
}
