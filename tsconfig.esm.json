{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "plugins": [
      {
        "transform": "@zerollup/ts-transform-paths",
        "after": true
      },
      {
        "transform": "@zerollup/ts-transform-paths",
        "afterDeclarations": true
      },
      {
        "transform": "@zoltu/typescript-transformer-append-js-extension/output/index.js",
        "after": true
      },
      {
        "transform": "@zoltu/typescript-transformer-append-js-extension/output/index.js",
        "afterDeclarations": true
      }
    ]
  },
  "exclude": [
    "examples",
    "node_modules",
    "src/**/*.test.ts",
    "src/**/*.browser-test.ts",
    "tools",
    "cjs",
    "esm",
    "build",
    "exports.js",
    "exports.d.ts",
    "exports.js.map",
    "exports.d.ts.map",
    "Console",
    "Disposable",
    "Effect",
    "Fiber",
    "Future",
    "Fx",
    "Key",
    "Patch",
    "Path",
    "Queue",
    "RemoteData",
    "Resume",
    "Scheduler",
    "Shared",
    "Storage",
    "Stream",
    "Uri",
    "Uuid",
    "browser",
    "common",
    "dom",
    "history",
    "http",
    "io",
    "lambda",
    "logging",
    "logic",
    "node"
  ]
}
