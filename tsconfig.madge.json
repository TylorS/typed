    "packages/route/build/examples/**/*",
    "packages/template/build/esm/**/*",
    "packages/template/build/test/**/*",
    "packages/template/build/examples/**/*",
    "packages/wire/build/esm/**/*",
    "packages/wire/build/test/**/*",
    "packages/wire/build/examples/**/*"
